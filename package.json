{
  "name": "ku",
  "displayName": "Ku",
  "description": "Ku Programming Language Support",
  "version": "0.0.1",
  "publisher": "zhaopuming",
  "repository": {
    "type": "git",
    "url": "https://github.com/ku-lang/vscode.git"
  },
  "engines": {
    "vscode": "^1.39.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "ku",
        "aliases": [
          "Ku",
          "Kulang",
          "ku",
          "kulang"
        ],
        "extensions": [
          ".ku"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "ku",
        "scopeName": "source.ku",
        "path": "./syntaxes/ku.tmLanguage.json"
      }
    ]
  }
}
